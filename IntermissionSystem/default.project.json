{
  "name": "IntermissionSystem",
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Game" : {
        "$className": "Folder",
        "Components": {
          "$className": "Folder"
        },
        "Services": {
          "$className": "Folder",
          "$path": "src/timerserver"
  
        },
        "Modules": {
          "$className": "Folder"
        },
        "KnitServerRuntime": {
          "$path": "src/server"
        }
      }
    },


    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Knit": {
        "$path": "src/Knit"
      }
    },

    "Workspace": {
      "$className": "Workspace",
      "Ingame" : {
        "$className": "Part",
        "$properties": {
          "Position": [-52.6, 0.5, -58.5]
        }
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Game" : {
          "$className": "Folder",
          "Components": {
            "$className": "Folder"
          },
          "Controllers": {
            "$className": "Folder",
            "$path": "src/TimerController"
          },
          "Modules": {
            "$className": "Folder"
          },
          "KnitClientRuntime": {
            "$path": "src/client"
          }
        }
      }
    },

    
    "StarterGui": {
      "$className": "StarterGui",
      "IntermissionGui": {
        "$className": "ScreenGui",
        "IntermissionTimer" : {
          "$className": "TextLabel",
          "$properties": {
            "BackgroundTransparency" : 1,
            "Position": [0.421, 0,0, 0],
            "Size": [0.158, 0 , 0.088 , 0],
            "TextScaled": true
          }
        }
      }
    }
  }
}